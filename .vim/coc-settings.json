{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod"
      ],
      "filetypes": [
        "go"
      ]
    },
    "terraform": {
      "command": "terraform-ls",
      "args": [
        "serve"
      ],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {}
    }
  },
  "go.goplsOptions": {
    "staticcheck": true
  },
  "coc.preferences.formatOnSaveFiletypes": ["rust"],
  "javascript.suggestionActions.enabled": false,
  "rust.wait_to_build": 200,
  "rust.clippy_preference":"on",
  "cSpell.enabledLanguageIds": [
    "markdown"
  ]
}
